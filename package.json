{
  "name": "next-serverless-poc",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "next build ./src",
    "deploy:beta": "yarn run build && cross-env NODE_ENV=beta sls deploy",
    "deploy:prod": "yarn run build && cross-env NODE_ENV=prod sls deploy",
    "dev": "next ./src",
    "start": "yarn build && cross-env NODE_ENV=prod node ./src/server"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "next": "^8.0.4",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "serverless": "^1.41.1"
  }
}
